@model IEnumerable<Test1.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Student_email)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Student_email)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Student_email }) |
                @Html.ActionLink("Details", "Details", new { id = item.Student_email }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Student_email})
            </td>
        </tr>
    }
